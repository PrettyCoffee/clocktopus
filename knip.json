{
  "$schema": "https://unpkg.com/knip@5/schema.json",
  "entry": [
    "src/main.tsx",
    "eslint.config.mjs",
    "tailwind/tailwind.config.ts"
  ],
  "ignore": ["**/*.d.ts", "**/tab-pattern.tsx", "**/auto-animate-height.tsx"],
  "project": ["./**/*.{ts,tsx,js,mjs}"],
  "paths": {
    "*": ["src/*"]
  },
  "ignoreBinaries": ["eslint"],
  "ignoreDependencies": [".*eslint.*"]
}
